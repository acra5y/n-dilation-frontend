    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "node-dev": "^4.0.0",
    "prettier": "^1.17.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "express": "^4.16.4",
